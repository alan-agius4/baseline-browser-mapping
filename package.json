{
  "name": "baseline-browser-mapping",
  "version": "2.5.0",
  "description": "A library for obtaining browser versions with their maximum supported Baseline feature set and Widely Available status.",
  "exports": {
    ".": "./dist/index.js",
    "./fetch": "./dist/baseline-browser-mapping.js"
  },
  "files": [
    "dist/*",
    "LICENSE.txt",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./fetch": [
        "./dist/baseline-browser-mapping.d.ts"
      ]
    }
  },
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "rm -rf dist; prettier . --write; rollup -c; cp dist/index.d.ts dist/baseline-browser-mapping.d.ts",
    "refresh-downstream": "npx tsx scripts/refresh-downstream.ts",
    "refresh-static": "npx tsx scripts/refresh-static.ts"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@mdn/browser-compat-data": "^6.0.22",
    "web-features": "^2.38.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^24.0.0",
    "prettier": "^3.5.3",
    "rollup": "^4.41.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-modify": "^3.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "repository": "git+https://github.com/web-platform-dx/baseline-browser-mapping.git"
}
